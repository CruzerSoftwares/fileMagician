{
    "name": "cruzersoftwares/filemagician",
    "description": "Web based file browser and editor for text and images",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "file-brower",
        "image-cropper",
        "markdown-editor",
        "file-manager",
        "filemanager",
        "online-editor",
        "html-editor",
        "browser-editor"
    ],
    "homepage": "https://cruzersoftwares.github.io/fileMagician/",
    "support": {
        "issues": "https://github.com/CruzerSoftwares/fileMagician/issues"
    },
    "require": {
        "php": ">=7",
        "gitonomy/gitlib": "^1.0",
        "twig/twig": "^2.0",
        "nikic/fast-route": "^1.3"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Models/Router.php", "app/Models/functions.php"]
    }
}
